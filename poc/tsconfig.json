    "compilerOptions": {
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "bin",
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "lib": ["es2015"]
}
